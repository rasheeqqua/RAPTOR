{
  "name": "raptor",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "ts-patch install -s",
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:manager": "nest start --watch",
    "start:engine": "nest start --watch --entryFile main-engine",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:prod:manager": "node dist/src/main",
    "start:prod:engine": "node dist/src/main-engine",
    "lint": "eslint \"{src,apps,libs}/**/*.ts\" --fix",
    "test": "vitest run --coverage"
  },
  "dependencies": {
    "@nestia/core": "3.12.0",
    "@nestia/fetcher": "3.12.0",
    "@nestia/sdk": "3.12.0",
    "@nestjs/common": "10.4.1",
    "@nestjs/config": "3.2.3",
    "@nestjs/core": "10.4.1",
    "@nestjs/microservices": "10.4.1",
    "@nestjs/platform-express": "10.4.1",
    "@nestjs/swagger": "7.4.0",
    "amqp-connection-manager": "4.1.14",
    "amqplib": "0.10.9",
    "brotli": "1.3.3",
    "express": "4.19.2",
    "minio": "8.0.6",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "scram-node": "file:./scram-node",
    "tmp": "0.2.3",
    "typescript": "5.5.4",
    "typia": "6.9.0",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "10.0.0",
    "@nestjs/testing": "10.4.1",
    "@swc/core": "1.15.3",
    "@types/amqplib": "0.10.7",
    "@types/express": "4.17.17",
    "@types/node": "20.3.1",
    "@types/supertest": "6.0.3",
    "@types/uuid": "9.0.8",
    "@typescript-eslint/eslint-plugin": "6.0.0",
    "@typescript-eslint/parser": "6.0.0",
    "@vitest/coverage-v8": "4.0.15",
    "eslint": "8.42.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.0",
    "prettier": "3.0.0",
    "rimraf": "5.0.0",
    "supertest": "7.1.4",
    "ts-loader": "9.4.3",
    "ts-node": "10.9.1",
    "ts-patch": "3.2.1",
    "tsconfig-paths": "4.2.0",
    "unplugin-swc": "1.5.9",
    "vitest": "4.0.15"
  }
}
